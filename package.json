{
  "name": "setup-superset-action",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "actions",
    "node",
    "setup",
    "superset"
  ],
  "main": "dist/run",
  "scripts": {
    "all": "npm run format && npm run lint && npm run test && npm run build",
    "build": "npm run clean && tsc && ncc build -o dist src/run.ts && ncc build -o dist/scripts/cache src/scripts/cache.ts",
    "clean": "rm -rf ./lib ./dist",
    "coverage": "npm run test && open ./coverage/lcov-report/index.html",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest --clearCache && jest --coverage"
  },
  "dependencies": {
    "@actions/cache": "actions/cache#78809b91d73d12e094c66654de7b65727733dcc6",
    "@actions/core": "1.2.3",
    "@actions/exec": "1.0.3",
    "@actions/glob": "0.1.0",
    "@types/uuid": "7.0.2",
    "hasha": "5.2.0",
    "tempy": "0.5.0",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "12.12.47",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "@zeit/ncc": "0.22.1",
    "eslint": "7.0.0",
    "eslint-plugin-jest": "23.8.2",
    "jest": "26.1.0",
    "js-yaml": "3.13.1",
    "prettier": "2.0.5",
    "prettier-plugin-packagejson": "2.2.0",
    "ts-jest": "26.1.1",
    "typescript": "3.8.3"
  }
}
